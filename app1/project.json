{
  "root": "app1",
  "sourceRoot": "app1/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["../dist/app1"],
      "options": {
        "maxWorkers": 1,
        "memoryLimit": 10281,
        "command": "webpack --config webpack.prod.js",
        "cwd": "app1"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "maxWorkers": 1,
        "tsConfig": "app1/tsconfig.json",
        "memoryLimit": 10281,
        "command": "webpack-dev-server",
        "cwd": "app1"
      }
    }
  },
  "tags": []
}
