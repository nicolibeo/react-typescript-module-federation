{
  "root": "apps/app2",
  "sourceRoot": "apps/app2/src",
  "projectType": "application",
  "targets": {

    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "baseHref": "/",
        "outputPath": "dist/apps/app2",
        "webpackConfig": "apps/app2/webpack.nx.js",
        "index": "apps/app2/public/index.html",
        "main": "apps/app2/src/main.tsx",
        "tsConfig": "apps/app2/tsconfig.app.json",
        "assets": [],
        "maxWorkers": 1,
        "styles": [],
        "scripts": [],
        "buildLibsFromSource": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/app2/src/environments/environment.ts",
              "with": "apps/app2/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "app2:build",
        "port": 3002,
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "app2:build:production",
          "hmr": false
        }
      }
    },
    "webpack": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "webpack-dev-server --config webpack.config.js",
        "cwd": "apps/app2"
      }
    },
    "webpack:build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "webpack --config webpack.config.js",
        "cwd": "apps/app2"
      }
    }
  },
  "tags": []
}
