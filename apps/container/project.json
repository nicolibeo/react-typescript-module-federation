{
  "root": "apps/container",
  "sourceRoot": "apps/container/src",
  "projectType": "application",
  "targets": {

    "build": {
      "executor": "@nrwl/web:webpack",
      "options": {
        "outputPath": "dist/apps/container",
        "webpackConfig": "apps/container/webpack.config.js",
        "index": "apps/container/public/index.html",
        "main": "apps/container/src/index.ts",
        "tsConfig": "apps/container/tsconfig.app.json",
        "assets": [],
        "maxWorkers": 1,
        "styles": [],
        "scripts": [],
        "buildLibsFromSource": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/container/src/environments/environment.ts",
              "with": "apps/container/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            }
          ]
        }
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "container:build",
        "port": 3000,
        "liveReload": false
      },
      "configurations": {
        "production": {
          "buildTarget": "container:build:production"
        }
      }
    }
  },
  "tags": []
}
